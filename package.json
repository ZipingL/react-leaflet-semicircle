{
    "name": "react-leaflet-semicircle",
    "version": "3.0.3",
    "description": "React wrapper of leaflet-semicircle",
    "main": "dist/index.js",
    "module": "src/index.js",
    "scripts": {
        "test": "jest",
        "cover": "jest --coverage --collectCoverageFrom=src/**/*.js",
        "lint": "eslint ./src/**/*.js ./__tests__/**/*.js",
        "changelog": "auto-changelog -p --tag-prefix v",
        "release": "np",
        "version": "npm run changelog && git add CHANGELOG.md",
        "build": "babel src --out-dir dist",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clementallen/react-leaflet-semicircle.git"
    },
    "author": "Clement Allen (https://clementallen.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/clementallen/react-leaflet-semicircle/issues"
    },
    "homepage": "https://github.com/clementallen/react-leaflet-semicircle#readme",
    "keywords": [
        "react",
        "leaflet",
        "react-leaflet",
        "react-leaflet-v3",
        "leaflet-semicircle",
        "react-leaflet-semicircle"
    ],
    "dependencies": {
        "leaflet-semicircle": "^2.0.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-react": "^7.14.5",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
        "auto-changelog": "^2.3.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "enzyme": "^3.11.0",
        "eslint": "^7.19.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.26.1",
        "jest": "^26.6.3",
        "leaflet": "^1.7.1",
        "np": "^7.5.0",
        "prettier": "^2.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-leaflet": "^3.2.2"
    },
    "peerDependencies": {
        "leaflet": "^1.0.0",
        "react": "^15.0.0 || ^16.0.0 || ^17.0.0",
        "react-dom": "^15.0.0 || ^16.0.0 || ^17.0.0",
        "react-leaflet": "^3.0.0"
    }
